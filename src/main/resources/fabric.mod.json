{
  "schemaVersion": 1,
  "id": "createframed",
  "version": "${version}",
  "name": "Create: Framed",
  "description": "A Create mod addon that allows for more colour customization with Framed Glass!",
  "authors": [
    "DakotaPride"
  ],
  "contact": {
    "sources": "https://github.com/DakotaPride/create-framed-fabric"
  },
  "license": "MIT",
  "icon": "assets/createframed/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.dakotapride.createframed.CreateFramedMod"
    ]
  },

  "accessWidener": "createframed.accesswidener",

  "mixins": [
    "createframed.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
